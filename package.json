{
  "name": "reka-dashboard",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "VITE_VUE_DEV_TOOLS=true vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "bun lint && vitest run",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "lint:staged": "eslint --pass-on-no-patterns --no-warn-ignored $(git diff --cached --name-only --diff-filter=d)",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "eslint --inspect-config",
    "precommit": "bun lint:staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "@internationalized/date": "^3.8.0",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/components": "^12.8.2",
    "@vueuse/core": "^12.8.2",
    "@vueuse/integrations": "^12.8.2",
    "@vueuse/router": "^12.8.2",
    "change-case": "^5.4.4",
    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.6.0",
    "non.geist": "^1.0.4",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "reka-ui": "^2.2.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1",
    "vue-sonner": "^1.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@tailwindcss/typography": "^0.5.16",
    "@tsconfig/node22": "^22.0.1",
    "@types/common-tags": "^1.8.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.18",
    "@unovis/ts": "^1.5.1",
    "@unovis/vue": "^1.5.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "class-variance-authority": "^0.7.1",
    "common-tags": "^1.8.2",
    "eslint": "^9.27.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vue": "^9.33.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "simple-git-hooks": "^2.13.0",
    "storybook": "^8.6.14",
    "storybook-vue3-router": "^5.0.0",
    "tw-animate-css": "^1.3.0",
    "typescript": "~5.7.3",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "bun precommit"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
