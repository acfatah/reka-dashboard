{
  "name": "vue-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "bun lint && vitest run",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "lint:staged": "eslint --pass-on-no-patterns --no-warn-ignored $(git diff --cached --name-only --diff-filter=d)",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "eslint --inspect-config",
    "precommit": "bun lint:staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@vueuse/components": "^12.7.0",
    "@vueuse/core": "^12.7.0",
    "@vueuse/integrations": "^12.7.0",
    "@vueuse/router": "^12.7.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-onboarding": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@storybook/vue3": "^8.5.8",
    "@storybook/vue3-vite": "^8.5.8",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.20.1",
    "eslint-plugin-storybook": "^0.11.3",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^8.5.8",
    "typescript": "~5.7.3",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-vue-components": "^28.4.0",
    "vite": "^6.1.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.6",
    "vue-tsc": "^2.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "bun precommit"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
